{
  "manifest_version": 3,
  "name": "Power Pages Helper",
  "version": "1.36",
  "description": "Enhances the Power Pages UI.",
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "https://make.powerpages.microsoft.com/*",
        "https://make.powerpages.microsoft.cn/*"
      ],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}